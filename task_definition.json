{
  "containerDefinitions": [
    {
      "memoryReservation": 600,
      "environment": [
        {
          "name": "RAILS_ENV",
          "value": "production"
        },
        {
          "name": "RACK_ENV",
          "value": "production"
        }
      ],
      "name": "sample-rails",
      "mountPoints": [],
      "image": "577425812578.dkr.ecr.ap-northeast-1.amazonaws.com/sample-image:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "service",
          "awslogs-group": "sample-service"
        }
      },
      "cpu": 333,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 3000,
          "hostPort": 0
        }
      ],
      "command": ["bundle", "exec", "pumactl", "start"],
      "essential": true,
      "volumesFrom": []
    }
  ]
}
